  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Launch via NPM",
          "runtimeExecutable": "npm",
          "runtimeArgs": [
              "run-script",
              "debug"
          ],
          "port": 9229
      },
      {
          "name": "Attach to Process",
          "type": "go",
          "request": "attach",
          "mode": "local",
          "processId": 0
      },
      {
          "name": "Launch Package",
          "type": "go",
          "request": "launch",
          "mode": "debug",
          "program": "${MilestoneProject1}"
      },
      {
          "name": "Launch test package",
          "type": "go",
          "request": "launch",
          "mode": "test",
          "program": "${MilestoneProject1}"
      },
      {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${MilestoneProject1}",
            "console": "internalConsole"
        }
  ]
}
